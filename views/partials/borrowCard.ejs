<!-- Nutzerkarte -->
<div class="card">
    <div class="card-body">
        <h5 class="card-title">
            <%= borrow.id %>
        </h5>
        <p class="card-text">Ausleiher: <a href="/users/<%=borrow.userid%>">
                <%= borrow.userid %>
            </a>
        </p>
        <p class="card-text">Equipment: <% borrow.equipmentids.forEach((equipmentid)=> { %>
                <a href="/equipment/<%=equipmentid%>">
                    <%=equipmentid%>
                </a>
                <%});%>
        </p>
        <p class="card-text">Startdatum: <%= borrow.start %>
        </p>
        <p class="card-text">Enddatum: <%= borrow.end %>
        </p>
